    </KinBody>
    <KinBody name="Table6" file="data/ikeatable.kinbody.xml">
        <Translation>3.5 1.2 0.74</Translation>
    </KinBody>
    <KinBody name="TibitsBox1">
        <Translation>3.5 -1.3 0.74</Translation>
        <Body type="dynamic">
            <Mass type="mimicgeom">
                <total>0.285</total>
            </Mass>
            <!-- Reference point will be at the base of the box -->
